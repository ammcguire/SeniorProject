<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ceo.procrastinaut">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.orm.SugarApp"
        >

        <activity
            android:name=".HomeScreen"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WeeklySchedule"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_weekly_schedule"
            android:parentActivityName=".HomeScreen"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".Assignments"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_assignments"
            android:parentActivityName=".HomeScreen"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".ProgressReports"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_progress_reports"
            android:parentActivityName=".HomeScreen"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".settings"
            android:parentActivityName=".HomeScreen"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_settings"
            android:theme="@style/FullscreenTheme"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data android:name="DATABASE" android:value="procrastinaut.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.example" />

    </application>



</manifest>